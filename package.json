{
  "name": "express-typescript",
  "version": "0.0.1-alpha0.0.1",
  "description": "Aplicacion de node.js con express y typescript",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "tsc:config": "npm run tsc -- --init",
    "dev": "tsnd --respawn src/index.ts",
    "build": "npm run tsc",
    "node:start": "node build/index.js",
    "clean": "rimraf build",
    "start": "npm-run-all clean build node:start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elboqueronpaco/express-typescript.git"
  },
  "keywords": [
    "javascript",
    "typescript",
    "node",
    "express"
  ],
  "author": "elboqueronpaco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elboqueronpaco/express-typescript/issues"
  },
  "homepage": "https://github.com/elboqueronpaco/express-typescript#readme",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/mongoose-unique-validator": "^1.0.6",
    "@types/node": "18.7.13",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "ts-node-dev": "2.0.0",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "2.8.5",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "express-validator": "6.14.2",
    "mongoose": "6.5.4",
    "mongoose-unique-validator": "3.1.0"
  }
}
